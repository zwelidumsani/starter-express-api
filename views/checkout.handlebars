 <!-- Page Header Start -->
    <div class="page-header" style="background: url(assets/img/2.jpg);">
      <div class="container">
        <div class="row">         
          <div class="col-md-12">
            <div class="breadcrumb-wrapper">
              <h2 class="page-title">{{headin}}</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Page Header End -->
	
	
	
	
	<ul id = "myTab" class = "nav nav-tabs navigation">
     <li><a href = "/"><i class = "lnr lnr-home"></i> Home</a></li>
	 <li><a href = "#"> About Us</a></li>
	 <li><a href = "/treatment"><i class = "lnr lnr-list"></i> Herbal Nutrition</a></li>
    <li><a href = "/relations"><i class = "lnr lnr-list"></i> Love & Relations</a></li>
	<li><a href = "/wealth"><i class = "lnr lnr-list"></i> Wealth & Success</a></li>
